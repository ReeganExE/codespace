{
  "name": "Code Space",
  "content_scripts": [
    {
      "matches": ["${BITBUCKET_ADDR}/*"],
      "js": ["background.js"]
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
